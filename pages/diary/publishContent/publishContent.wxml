<view class='publishContent'>
<textarea class='publishText' placeholder="请输入内容"></textarea>

<view class='forumImgs'>
<block wx:for="{{editImages}}">
 <image class="forumItemImage" wx:if="{{item}}" src="{{item}}" mode='aspectFill'></image>
</block>
   
</view>
<view class='publishSelectBar'>
  <image class='selectBarImg' src='/common/icons/iconImage.png' bindtap='getImageTap'></image>
  <button class='publishPost' size="mini" >发布</button>
</view>
</view>


